<launch>
    <!-- 设置地图的配置文件 -->
    <arg name="map" default="nav_ground.yaml" />
    <!-- 运行地图服务器，并且加载设置的地图-->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find game_demo)/nav/map/$(arg map)"/>
    <!-- 启动AMCL节点 -->
    <include file="$(find game_demo)/nav/launch/04_amcl.launch" />
    <!-- 运行move_base节点 -->
    <include file="$(find game_demo)/nav/launch/05_move_base.launch" />
    <!-- 运行rviz -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find game_demo)/nav/config/map_02.rviz" />
    <node pkg="robot_state_publisher" name="robot_state_publisher" type="robot_state_publisher" />
    <node pkg="joint_state_publisher" name="joint_state_publisher" type="joint_state_publisher" />
</launch>